snippet clg "console.log()" b
console.log(${1:'$2', $3});
endsnippet

snippet for "for" b
for(let i = $1; i < $2; i++) {
	$3
}
endsnippet

snippet pub "public" b
public $1:$2
endsnippet

snippet fn "function" b
$1($2): ${3:void} {
	$4
}
endsnippet

snippet des "desestructuring" b
const { $1 } = $2;
endsnippet

snippet clone "clone" b
const $1 = {...$2};
endsnippet

snippet afn "arrow function" b
const $1 = ($2) => {
  $3
}
endsnippet
